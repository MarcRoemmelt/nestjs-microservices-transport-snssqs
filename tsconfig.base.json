{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "target": "ESNext",
    "lib": ["ESNext"],
    "declaration": true,
    "noImplicitAny": false,
    "noImplicitOverride": false,
    "removeComments": true,
    "noLib": false,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "outDir": "./dist",
    "rootDir": ".",
    "skipLibCheck": true,
    "baseUrl": "./"
  }
}
